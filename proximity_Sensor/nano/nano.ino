    float accZ = axisValues[2];

    // Calculate pitch and roll angles from accelerometer data
    pitch = atan2(-accX, sqrt(accY * accY + accZ * accZ)) * 180.0 / PI;
    roll = atan2(accY, accZ) * 180.0 / PI;
  }

  // Calculate the absolute value of pitch and roll
  int absolutePitch = abs(pitch);
  int absoluteRoll = abs(roll);
  
  // Map the pitch and roll angles to pwm values range 750-2250
  // front-back
  if(absolutePitch>absoluteRoll){
    servoLeftAngle = map(pitch, -90, 90, 2250, 750);
    servoRightAngle = map(pitch, -90, 90, 750, 2250);
  }

  // left-right
  else if(absoluteRoll>absolutePitch){
    servoLeftAngle = map(roll, -90, 90, 750, 2250);
    servoRightAngle = map(roll, -90, 90, 750, 2250);
  }

  // enable servos
  if((absolutePitch>30)||(absoluteRoll>30)){
    servoLeft.attach(servoLeftPin);
    servoRight.attach(servoRightPin);
  }

  // disable servos
  else if((absolutePitch<=30)&&(absoluteRoll<=30)){
    servoLeft.detach();
    servoRight.detach();
  }

  // Constrain servo angles to valid range
  servoLeftAngle = constrain(servoLeftAngle, 750, 2250);
  servoRightAngle = constrain(servoRightAngle, 750, 2250);

  // Set servo positions
  servoLeft.write(servoLeftAngle);
  servoRight.write(servoRightAngle);

  delay(20); // Adjust delay as needed
}

void lights(){
  // read the sensor:
  lightValue = analogRead(photocell);

  if (lightValue >= darknessThreshold) {
    digitalWrite(LED_PIN,LOW);
  } 
  else if (lightValue < darknessThreshold) {
    digitalWrite(LED_PIN,HIGH);
  }
  delay(20);
}
