const int sensorMin = 10;
const int sensorMax = 1023;

// Desired output range in Hz
const int outputMin = 800;
const int outputMax = 1500;

#define LED_PIN 12
#define BUTTON_PIN 2

volatile byte alarmState = HIGH;

const int servoLeftPin = 9;   // Digital pin for left servo
const int servoRightPin = 10; // Digital pin for right servo

